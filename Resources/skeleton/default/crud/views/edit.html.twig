{{ '{% extends \'::backend_base.html.twig\' %}' }}

{{ '{% block tituloSeccion %}' }}
{{ '{{ \'Edit %entity_name%\' }}'|replace({'%entity_name%':entity}) }}
{{ '{% endblock %}' }}

{{ '{% block contenido %}' }}
<div class="row span16" style="margin: 0 auto">
    <div class="actions_list">
        <a href="{{ "{{ path('"~ route_name_prefix ~"') }}" }}" class="btn info">{{ '{{ \'List\'|trans }}' }}</a>
        <a href="{{ "{{ path('"~ route_name_prefix ~"_new') }}" }}" class="btn info">{{ '{{ \'New\'|trans }}' }}</a>
        <a href="{{ "{{ path('"~ route_name_prefix ~"_show', { 'id': entity.id }) }}" }}" class="btn info">{{ '{{ \'Show\'|trans }}' }}</a>
        <a href="#" onclick="confirmarUrl('{{ '{{ \'Realmente desea borrar\'|trans }}' }}', '{{ "{{ path('"~ route_name_prefix ~"_delete', { 'id': entity.id }) }}" }}')" class="btn danger">{{ '{{ \'Delete\'|trans }}' }}</a>
    </div>
    {{ '{% form_theme form \'CULabsAdminBundle:Form:fields.html.twig\' %}' }}
    <form id="form_new" action="#" method="post" {{ '{{ form_enctype(form) }}' }}>
        <div class="form">
            {{ "{{ form_widget(form) }}" }}
            <div class="actions_form">	
                <input class="btn primary" value="{{ '{{ \'Save\'|trans }}' }}" type="submit"/>
            </div>
        </div>
    </form>
</div>
{{ '{% endblock %}' }}
